page {
  border-color: #f6f6f4;
}
.user_info .user_bg {
  position: relative;
  height: 40vh;
  background-color: var(--themeColor);
}
.user_info .user_bg .wrap {
  position: relative;
  top: 30%;
  text-align: center;
}
.user_info .user_bg .wrap image {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
}
.user_info .menu {
  margin-top: 15rpx;
  background-color: #f6f6f4;
}
.user_info .menu .order_wrap {
  margin: 15rpx;
  background-color: #fff;
}
.user_info .menu .order_wrap .title {
  padding: 15rpx;
  padding-left: 35rpx;
  border-bottom: #f6f6f4;
}
.user_info .menu .order_wrap .content {
  display: flex;
  text-align: center;
  padding: 20rpx;
}
.user_info .menu .order_wrap .content navigator {
  flex: 1;
  padding: 15rpx 0;
}
.user_info .menu .order_wrap .content navigator .iconfont {
  font-size: 24px;
}
.user_info .menu .address_wrap {
  margin: 15rpx;
  margin-top: 20rpx;
  background-color: #fff;
  padding: 20rpx 0;
  padding-left: 35rpx;
}
.user_info .menu .app_wrap {
  margin: 15rpx;
  margin-top: 20rpx;
  background-color: #fff;
}
.user_info .menu .app_wrap .item {
  padding: 20rpx;
  padding-left: 35rpx;
  border-bottom: 5rpx solid #f6f6f4;
}
.user_info .menu .app_wrap .contact {
  display: flex;
  justify-content: space-evenly;
}
.user_info .menu .app_wrap .feedback {
  margin: 0;
  padding: 0;
  border-color: transparent;
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: start;
  align-items: center;
  font-size: 28rpx;
  font-weight: normal;
  flex-grow: 1;
}
.user_info .menu .app_wrap .feedback::after {
  border: none;
}
